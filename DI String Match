class Solution:
    def diStringMatch(self, S: 'str') -> 'List[int]':
        inc = 0
        dec = len(S)
        arr = [0] * (len(S)+ 1)
        temp = [char for char in S]
        for i in range(len(temp)):
            if temp[i] == 'I':
                arr[i] = inc
                inc += 1
            else:
                arr[i] = dec
                dec -= 1
        arr[len(temp)] = inc      
        return arr
