class Solution:
    def findComplement(self, num: int) -> int:
        b = "{0:b}".format(num)
        b = int(b)
        arr = []
        for item in str(b):
            if item == '1':
                arr.append(0)
            else:
                arr.append(1)
        temp = [str(i) for i in arr]        
        comp = ''.join(temp)
        return int(comp, 2)
